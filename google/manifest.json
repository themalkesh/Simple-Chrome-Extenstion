{
    "manifest_version": 2,
    "name": "Google Search",
    "version": "0.1",
    "browser_action": {
         
        "default_popup": "popup.html",
        "default_title": "Click here!"
      },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["jquery-3.2.1.min.js","content.js"]
        }
      ]
  }